     <div class="col-lg-6 col-lg-offset-3 centrate">
<h2>Log in</h2>
<% console %>
<hr>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.email_field :email, autofocus: true, 'placeholder':'Email', 'style':'width:100%;' %>
  </div>
<br>
  <div class="field">
    <%= f.password_field :password, autocomplete: "off", 'placeholder':'Password', 'style':'width:100%;' %>
  </div>
<br>
  
  <div class="actions">
    <%= f.submit "Log in" , 'style':'width:100%;', 'class': 'btn btn-success'%>
  </div>
<% end %>
<br>
<%= render "devise/shared/links" %>
</div>